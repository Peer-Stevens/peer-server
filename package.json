{
	"name": "peer-server",
	"version": "1.0.0",
	"description": "server for Peer",
	"main": "src/server.ts",
	"repository": "https://github.com/Peer-Stevens/peer-server.git",
	"author": "Andrew Jones <ajones4@stevens.edu>",
	"license": "UNLICENSED",
	"private": true,
	"scripts": {
		"build": "tsc",
		"watch": "nodemon --exec ts-node -- ./src/server.ts",
		"dev": "ts-node ./src/server.ts",
		"start": "node ./dist/server.js",
		"prod": "yarn run build && yarn run start",
		"prepare": "husky install",
		"lint": "pretty-quick --staged && lint-staged",
		"seed": "ts-node ./src/seed/seed.ts",
		"test": "yarn jest"
	},
	"dependencies": {
		"@googlemaps/google-maps-services-js": "^3.2.2",
		"axios": "^0.24.0",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"express-rate-limit": "^5.5.0",
		"mongodb": "^4.1.2",
		"typescript": "^4.4.3"
	},
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/jest": "^27.0.2",
		"@types/express-rate-limit": "^5.1.3",
		"@types/node": "^16.9.1",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.24.2",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.2",
		"jest": "^27.3.1",
		"lint-staged": "^11.1.2",
		"nodemon": "^2.0.12",
		"prettier": "^2.4.0",
		"pretty-quick": "^3.1.1",
		"ts-jest": "^27.0.7",
		"ts-node": "^10.2.1"
	},
	"lint-staged": {
		"*.{ts,js}": "eslint --cache --fix"
	}
}
